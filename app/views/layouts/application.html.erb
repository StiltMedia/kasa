<!DOCTYPE html>
<html>
<head>
  <title>Kasa</title>
  <%= stylesheet_link_tag    'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css'  %>
  <%= stylesheet_link_tag    'https://fonts.googleapis.com/css?family=Lato:100,100italic,300,300italic,400,400italic,700,700italic,900italic,900' %>
  <%= stylesheet_link_tag    'https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css' %>
  <%= stylesheet_link_tag    asset_url('style.css'), media: 'all' %>  
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%#= stylesheet_link_tag    'http://s3-us-west-2.amazonaws.com/pesticide/1.2.0/pesticide.min.css', media: 'all' %>
  <%= stylesheet_link_tag    'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/5.3.2/css/bootstrap-slider.css' %>
  <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.3/owl.carousel.css' %>
  <%= stylesheet_link_tag 'https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.3/owl.theme.css' %>

  <%= csrf_meta_tags %>
</head>
<%
is_admin_page = nil
is_admin_page = true if params[:controller] == "tickets"
is_admin_page = true if params[:controller] == 'pages' && params[:action] == 'user_dashboard'
%>

<body class="<%= 'map-view' if session[:browse_view]  == 'Map' && params[:action] == 'browse' %> <%= 'admin-pages' if is_admin_page %>">

<%= yield %>


<%= javascript_include_tag 'application' %>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js">
<%= javascript_include_tag 'custom' %>
<%= javascript_include_tag 'jquery.scrollTo.min.js' %>
<%= javascript_include_tag 'https://code.jquery.com/ui/1.10.4/jquery-ui.js' %>
<%= javascript_include_tag 'http://maps.googleapis.com/maps/api/js?libraries=places' %>

<%= javascript_include_tag 'imgLiquid-min' %>



<%= javascript_include_tag 'bootstrap-slider.js' %>
<%= javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/owl-carousel/1.3.3/owl.carousel.js' %>
</body>
</html>
