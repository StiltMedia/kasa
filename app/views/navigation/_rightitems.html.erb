<% if user_signed_in? %>
  Welcome <strong><%= current_user.email %></strong>.
  <%= link_to 'Edit profile', edit_user_registration_path, class: 'navi-links activo' %>
  <%= link_to "Logout", destroy_user_session_path, method: :delete, class: 'navi-links activo' %>
<% else %>
  <%= link_to "Log In", new_user_session_path, class: 'navi-links' %>
  <%= link_to "Sign Up", new_user_registration_path, class: 'navi-links activo' %>
<% end %>
