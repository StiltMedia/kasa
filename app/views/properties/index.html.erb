<p id="notice"><%= notice %></p>

<h1>Listing Properties</h1>

<table>
  <thead>
    <tr>
      <th>Mls listing number</th>
      <th>Mls name</th>
      <th>Mls sources</th>
      <th>Mls date added</th>
      <th>Mls date modified</th>
      <th>Street number</th>
      <th>Street name</th>
      <th>Unit number</th>
      <th>City</th>
      <th>Zip</th>
      <th>Location</th>
      <th>Full address</th>
      <th>Property type</th>
      <th>Last update description</th>
      <th>Short last update description</th>
      <th>Status</th>
      <th>Current list price</th>
      <th>Sold price</th>
      <th>Sqft</th>
      <th>Sqft price</th>
      <th>Lot sqft</th>
      <th>Year built</th>
      <th>Listing office</th>
      <th>Condition</th>
      <th>Bedrooms</th>
      <th>Half bathrooms</th>
      <th>Full bathrooms</th>
      <th>Favorited</th>
      <th>Fake favorited</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.mls_listing_number %></td>
        <td><%= property.mls_name %></td>
        <td><%= property.mls_sources %></td>
        <td><%= property.mls_date_added %></td>
        <td><%= property.mls_date_modified %></td>
        <td><%= property.street_number %></td>
        <td><%= property.street_name %></td>
        <td><%= property.unit_number %></td>
        <td><%= property.city %></td>
        <td><%= property.zip %></td>
        <td><%= property.location %></td>
        <td><%= property.full_address %></td>
        <td><%= property.property_type %></td>
        <td><%= property.last_update_description %></td>
        <td><%= property.short_last_update_description %></td>
        <td><%= property.status %></td>
        <td><%= property.current_list_price %></td>
        <td><%= property.sold_price %></td>
        <td><%= property.sqft %></td>
        <td><%= property.sqft_price %></td>
        <td><%= property.lot_sqft %></td>
        <td><%= property.year_built %></td>
        <td><%= property.listing_office %></td>
        <td><%= property.condition %></td>
        <td><%= property.bedrooms %></td>
        <td><%= property.half_bathrooms %></td>
        <td><%= property.full_bathrooms %></td>
        <td><%= property.favorited %></td>
        <td><%= property.fake_favorited %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
