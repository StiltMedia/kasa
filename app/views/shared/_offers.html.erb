					<!-- BEGIN PORTLET-->
					<div class="portlet light">
						<div class="portlet-title">
							<div class="caption caption-md">
								<i class="icon-bar-chart theme-font hide"></i>
								<span class="caption-subject theme-font bold uppercase">Offers / Activity</span>
								<span class="caption-helper hide">weekly stats...</span>
							</div>
							<div class="actions">
 
                                                          <div class="btn-group">
                                                            <a class="btn btn-transparent grey-salsa btn-outline btn-circle btn-sm" href="javascript:;" onclick="$('.dd').nestable('expandAll')" data-toggle="dropdown" data-hover="dropdown" data-close-others="true"> Expand All
                                                            <i class="fa zfa-thumbs-o-up"></i>
                                                            </a>
                                                            <a class="btn btn-transparent grey-salsa btn-outline btn-circle btn-sm" href="javascript:;" onclick="$('.dd').nestable('collapseAll')" data-toggle="dropdown" data-hover="dropdown" data-close-others="true"> Collapse All
                                                            <i class="fa zfa-thumbs-o-up"></i>
                                                            </a>

                                                </div>




							</div>
						</div>
						<div class="portlet-body">
                                                  <% 
                                                     nlist = Negotiation.all.group_by { |x| [x.seller_id, x.buyer_id, x.property_id] }
                                                  %>
						  <% nlist.each do |neg| %>
						     <% property = Property.find(neg[0][2]) %>
						     <div class="dd">
						       <ol class="dd-list">

							 <li class="dd-item">
                                                           <div class="dd-handle" style="height: 40px">
							     &nbsp;&nbsp;
                                                             <% if property.images_tot_available > 0 %>
                                                               <img class="user-pic" style="border-radius: 0%; height: 29px;" src="<%= "http://kasa-staging-02.s3.amazonaws.com/#{property.listing_id}_#{0}.jpg" %>">
                                                             <% else %>
                                                               <img class="user-pic" style="border-radius: 0%; height: 29px;" src="<%= "http://kasa-staging-02.s3.amazonaws.com/#{"FE9E0000"}_#{0}.jpg" %>">
							      <% end %>
							      &nbsp;&nbsp;
                                                              <span href="<%= "/pages/listing_details/#{property.listing_id}" %>" class="primary-link"><%= truncate(property.address, length: 17) %>
                                                              </span>
							      &nbsp;&nbsp;&nbsp;&nbsp;
							      <div class="label label-sm label-danger invisible">No offers </div>
							      <img alt="" class="img-circle" src="/users/<%= neg[0][0]%>/show_picture" height="25">
							      <img alt="" class="img-circle" src="/users/<%= neg[0][1]%>/show_picture" height="25">



							   </div>

                                                               

                                                              <ol class="dd-list">
                                                                <li class="dd-item" data-id="4">
                                                                   <div class="dd-handle">
                                                                      <div id="jstree_demo_div" class="jstree_demo_div">
                                                                        <ul>
									    <% neg[1].each do |n| %>
									      <% if n.ntype == "offered" %>
                                                                                <li data-jstree='{"icon":"fa fa-money"}'><span onclick="location='http://www.google.com/'" style="pointer-events: auto; border-bottom: 1px dotted silver; cursor: pointer;"><%= User.find(n.buyer_id).nick %></span> Offered  <%= number_to_currency(n.amount, precision: 0) %> to <%= User.find(n.seller_id).nick %></li>
                                                                              <% elsif n.ntype == "declined" %>
                                                                                <li data-jstree='{"icon":"fa fa-times-circle-o"}'><%= User.find(n.seller_id).nick %> Declined </li>
                                                                              <% elsif n.ntype == "baccepted" %>
                                                                                <li data-jstree='{"icon":"fa fa-flag-checkered"}'><%= User.find(n.buyer_id).nick %> Accepted </li>
                                                                              <% elsif n.ntype == "saccepted" %>
                                                                                <li data-jstree='{"icon":"fa fa-flag-checkered"}'><%= User.find(n.seller_id).nick %> Accepted </li>
                                                                              <% elsif n.ntype == "scountered" %>
                                                                                <li data-jstree='{"icon":"fa fa-mail-forward"}'><%= User.find(n.seller_id).nick %> Countered with <%= number_to_currency(n.amount, precision: 0) %> </li>
                                                                              <% elsif n.ntype == "bcountered" %>
                                                                                <li data-jstree='{"icon":"fa fa-flag-checkered"}'><%= User.find(n.buyer_id).nick %> Countered with <%= number_to_currency(n.amount, precision: 0) %></li>
									      <% else %>
                                                                                <li data-jstree='{"icon":"fa fa-wechat"}'><%= n.ntype %>  </li>
									      <% end %>
									    <% end %>

                                                                        </ul>
                                                                      </div>
                                                                   </div><br>
                                                                </li>
                                                              </ol>
						       </ol>
						     </div>
						  <% end %>
						  <br>

						</div>
					</div>
					<!-- END PORTLET-->
                                                                          
